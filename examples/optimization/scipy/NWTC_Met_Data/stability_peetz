    "bins = np.linspace(-1,1,18)\n",
    "#group = df_stability.groupby(pd.cut(df_stability.zL_sonic_roll_10min, bins))\n",
    "group = df_stability.groupby(pd.cut(df_stability.zL_10m_sonic_10min, bins))\n",
    "\n",
    "plot_centers_lidar = (bins [:-1] + bins [1:])/2\n",
    "plot_values_60m_lidar = group.TI_60m_lidar_10min.mean()\n",
    "plot_values_80m_lidar = group.TI_80m_lidar_10min.mean()\n",
    "plot_values_60m_cup = group.TI_60m_cup_10min.mean()\n",
    "\n",
    "#Perc_error=(plot_values_60_lidar.values-plot_values_60)/plot_values_60*100\n",
    "\n",
    "fig = plt.figure(figsize=(12,4))\n",
    "ax  = fig.add_subplot(121)\n",
    "ax.plot(plot_centers_lidar, plot_values_60m_lidar,'bo-',label='60m lidar')\n",
    "ax.plot(plot_centers_lidar, plot_values_60m_cup,'go-',label='60m cup')\n",
    "ax.plot(plot_centers_lidar, plot_values_80m_lidar,'ko-',label='80m lidar')\n",
    "plt.xlabel('z/L', fontsize=18)\n",
    "plt.ylabel('TI (%)', fontsize=18)\n",
    "ax.legend()\n",
    "plt.ylim(2.5, 25)\n",
    "plt.xlim(-0.5, 0.5)"
